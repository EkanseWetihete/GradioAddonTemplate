{
  "inputs": [
    {
      "type": "dropdown",
      "label": "AI Type",
      "name": "Chat_Model"
    },
	{
      "type": "dropdown",
      "label": "AI Model",
      "name": "PhilipTheGreat/DiabloGPT-small-Traveller DiabloGPT-medium",
	  "ai_models_location": "True"
    },
    {
      "type": "textbox",
      "label": "Input Text",
	  "placeholder": ""
    },
    {
      "type": "slider",
      "label": "Max Length",
      "min": 10,
      "max": 1000,
	  "default": 100,
	  "step": 1
    },
    {
      "type": "slider",
      "label": "No Repetition Size",
      "min": 1,
      "max": 10,
	  "default": 4,
	  "step": 1
    },
    {
      "type": "slider",
      "label": "Temperature",
      "min": 0.1,
      "max": 1.0,
	  "default": 0.7,
	  "step": 0.1
    },
    {
      "type": "slider",
      "label": "Top probability",
      "min": 0.1,
      "max": 1.0,
	  "default": 0.9,
	  "step": 0.1
    },
    {
      "type": "checkbox",
	  "label": "Randomness",
	  "Enabled": "True"
    }
  ]
}